IMPLEMENTATION MODULE OutTerminal; (* Ch. Jacobi, S.E. Knudsen; version 10.6.81 *)(*$T-*) (*$R-*)  FROM SYSTEM IMPORT WORD;  IMPORT Terminal;  PROCEDURE Write(ch: CHAR);  BEGIN Terminal.Write(ch)  END Write;         PROCEDURE WriteLn;  BEGIN Write(36C)  END WriteLn;  PROCEDURE WriteT(s: ARRAY OF CHAR; length: CARDINAL);    VAR l, h: CARDINAL;   BEGIN    l := 0; h := HIGH(s);    WHILE (l<=h) AND (s[l]<>0C) DO      Write(s[l]); INC(l)    END;    FOR h := l+1 TO length DO Write(" ") END  END WriteT;      PROCEDURE WriteI(value: INTEGER; length: CARDINAL);    VAR i,n: CARDINAL;        a: ARRAY [0..6] OF CHAR;  BEGIN    IF length=0 THEN Write(" ") END;    IF value=(-32767-1) THEN  i := 5;      a[0] := "8"; a[1] := "6"; a[2] := "7";      a[3] := "2"; a[4] := "3";    ELSE      i := 0; n := ABS(value);      REPEAT a[i] := CHAR(n MOD 10 + CARDINAL("0"));        n := n DIV 10; INC(i)      UNTIL n = 0;    END;    IF value<0 THEN a[i] := "-"; INC(i) END;    WHILE length>i DO Write(" "); DEC(length) END;    REPEAT DEC(i); Write(a[i]) UNTIL i=0  END WriteI;     PROCEDURE WriteC(value: CARDINAL; length: CARDINAL);    VAR i: CARDINAL;        a: ARRAY [0..6] OF CHAR;  BEGIN    IF length=0 THEN Write(" ") END;    i := 0;     REPEAT a[i] := CHAR(value MOD 10 + CARDINAL("0"));      value := value DIV 10; INC(i)    UNTIL value = 0;    WHILE length>i DO Write(" "); DEC(length) END;    REPEAT DEC(i); Write(a[i]) UNTIL i=0  END WriteC;    PROCEDURE WriteO(value: WORD; length: CARDINAL);    VAR i, n: CARDINAL;        a: ARRAY [0..6] OF CHAR;  BEGIN    IF length=0 THEN Write(" ") END;    i := 0; n := CARDINAL(value);    REPEAT a[i] := CHAR(n MOD 8 + CARDINAL("0"));      n := n DIV 8; INC(i)    UNTIL n=0;    WHILE length>6 DO Write(" "); DEC(length) END;    WHILE length>i DO Write("0"); DEC(length) END;    REPEAT DEC(i); Write(a[i]) UNTIL i=0  END WriteO;    END OutTerminal.