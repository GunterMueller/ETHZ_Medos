DEFINITION MODULE DumpFileHandler; (* Ch. Jacobi 3.6.81 *)  FROM SYSTEM IMPORT ADDRESS;  EXPORT QUALIFIED OpenDumpFile, Code, Mem, ReadMemBlock;  PROCEDURE Code(f, pc: CARDINAL): CARDINAL;    (*gets a codeword (8 bit) from location 2*f + pc DIV 2*)  PROCEDURE Mem(addr: ADDRESS): CARDINAL;    (*reads a word from dumpFile*)  PROCEDURE OpenDumpFile(query: BOOLEAN);  PROCEDURE ReadMemBlock(fp, n: CARDINAL; dest: ADDRESS);    (*reads n words at frame address fp from dumpFile *)END DumpFileHandler.